  "scripts": {
    "build": "npm run clean && npm run copy:fonts && npm run copy:images && npm run build:main && npm run build:main:min && npm run build:latest && npm run build:latest:min && npm run build:scoped && npm run build:scoped:min && npm run build:scoped-latest:min && npm run build:scoped-latest",
