    "build:main:min": "cross-var lessc -s --source-map --npm-import=\"prefix=~\" --clean-css=\"--s1 --advanced\" src/less/build.less dist/css/$npm_package_name.$npm_package_version.min.css",
    "build:scoped": "cross-var lessc -s --source-map --npm-import=\"prefix=~\" src/less/build-scoped.less dist/css/$npm_package_name.$npm_package_version.scoped.css",
    "build:scoped-latest": "cross-var lessc -s --source-map --npm-import=\"prefix=~\" src/less/build-scoped.less dist/css/$npm_package_name.latest.scoped.css",
    "build:scoped-latest:min": "cross-var lessc -s --source-map --npm-import=\"prefix=~\" -x src/less/build-scoped.less dist/css/$npm_package_name.latest.scoped.min.css",
    "build:scoped:min": "cross-var lessc -s --source-map --npm-import=\"prefix=~\" -x src/less/build-scoped.less dist/css/$npm_package_name.$npm_package_version.scoped.min.css",
    "clean": "rimraf dist",
    "copy:fonts": "cpx \"node_modules/pnnl-icon-font/src/fonts/*.*\" \"dist/fonts\"",
    "copy:images": "cpx \"src/images/*.*\" \"dist/images\""
  },
